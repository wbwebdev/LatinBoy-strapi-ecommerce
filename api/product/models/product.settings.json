{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "required": true,
      "type": "integer"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "stock": {
      "required": true,
      "type": "integer"
    },
    "categories": {
      "type": "enumeration",
      "enum": [
        "toys"
      ],
      "required": true
    }
  }
}
